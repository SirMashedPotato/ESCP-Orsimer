<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="VFECore.PatchOperationToggable">
	  <enabled>False</enabled>
	  <mods>
		<li>Royalty</li>
	  </mods>
	  <label>[MorrowRim - Orsimer] Make all Royalty stellic wardens Orsimer:</label>
	  <match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/PawnKindDef[defName="Empire_Fighter_StellicGuardMelee"]</xpath>
				<value>
					<race>MorrowRim_OrsimerRace</race>
				</value>
			</li>
		</operations>
	  </match>
	</Operation>
	
</Patch>

