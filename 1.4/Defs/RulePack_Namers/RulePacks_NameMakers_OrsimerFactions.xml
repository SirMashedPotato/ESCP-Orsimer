<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ========== [ESCP_Orsimer_NamerFaction] ========== -->
	<RulePackDef>
		<defName>ESCP_Orsimer_NamerFaction</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->Orsimer Kingdom of [FamilyList]</li>
			</rulesStrings>
			<rulesFiles>
				<li>FamilyList->ESCP_OrsimerNames/OrsimerFamily</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<!-- ========== [ESCP_Orsimer_NamerFactionTribe] ========== -->
	<RulePackDef>
		<defName>ESCP_Orsimer_NamerFactionTribe</defName>
		<rulePack>
			<include>
				<li>TribalWordUtility</li>
			</include>
			<rulesStrings>
				<li>r_name(p=2)->The [Color] [TerrainFeature] Orsimer</li>
				<li>r_name(p=1)->The [TerrainFeature] Orsimer</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	<!-- ========== [ESCP_Orsimer_NamerFactionRaider] ========== -->
	<RulePackDef>
		<defName>ESCP_Orsimer_NamerFactionRaider</defName>
		<include>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->The [Color] [TerrainFeature] Ravagers</li>
				<li>r_name->[FamilyList]'s Ravagers</li>
				<li>r_name->[OrsimerCities] Ravagers</li>
				<li>r_name->[FamilyList]'s Raiders</li>
				<li>r_name->[OrsimerCities] Raiders</li>
			</rulesStrings>
			<rulesFiles>
				<li>OrsimerCities->ESCP_OrsimerNames/OrsimerCities</li>
				<li>FamilyList->ESCP_OrsimerNames/OrsimerFamily</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<!-- ========== [ESCP_Orsimer_NamerSettlement] ========== -->
	<RulePackDef>
		<defName>ESCP_Orsimer_NamerSettlement</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->[OrsimerCities]</li>
				<li>r_name->[FamilyList] Stronghold</li>
				<li>r_name->[FamilyList] Fortress</li>
				<li>r_name->[FamilyList] Camp</li>
			</rulesStrings>
			<rulesFiles>
				<li>OrsimerCities->ESCP_OrsimerNames/OrsimerCities</li>
				<li>FamilyList->ESCP_OrsimerNames/OrsimerFamily</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<!-- -->
	<RulePackDef>
		<defName>ESCP_NamerFactionOrsimer</defName>
		<include>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=2)->The [Color] [TerrainFeature] Orsimer</li>
				<li>r_name(p=1)->The [TerrainFeature] Orsimer</li>
			</rulesStrings>
			<rulesFiles>
      </rulesFiles>
		</rulePack>
	</RulePackDef>
	<RulePackDef>
		<defName>ESCP_NamerFactionOrsimerRaider</defName>
		<include>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->The [Color] [TerrainFeature] Ravagers</li>
				<li>r_name->[FamilyList]'s Ravagers</li>
				<li>r_name->[OrsimerCities] Ravagers</li>
				<li>r_name->The [Color] [TerrainFeature] Raiders</li>
				<li>r_name->[FamilyList]'s Raiders</li>
				<li>r_name->[OrsimerCities] Raiders</li>
			</rulesStrings>
			<rulesFiles>
				<li>OrsimerCities->ESCP_OrsimerNames/OrsimerCities</li>
				<li>FamilyList->ESCP_OrsimerNames/OrsimerFamily</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
</Defs>
