<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>MorrowRim</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
			
				<!-- MorrowRim_Ashlands -->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="MorrowRim_Ashlands"]/wildAnimals</xpath>
					<value>
						<MorrowRim_Welwa>0.05</MorrowRim_Welwa>
					</value>			
				</li>
				
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="MorrowRim_Welwa"]</xpath>
					<value>
						<li Class="MorrowRim.ExtendedRaceProperties">
							<ashResistant>Resistant</ashResistant>
						</li>
					</value>
				</li>	

			</operations>
		</match>
	</Operation>
	
</Patch>

