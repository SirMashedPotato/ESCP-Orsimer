<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Nature's Pretty Sweet</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>

				<!-- TKKN_RedwoodForest -->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="TKKN_RedwoodForest"]/wildAnimals</xpath>
					<value>
						<ESCP_Echatere>0.2</ESCP_Echatere>
					</value>			
				</li>
				
				<!-- TKKN_Grasslands -->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="TKKN_Grasslands"]/wildAnimals</xpath>
					<value>
						<ESCP_Welwa>0.05</ESCP_Welwa>
					</value>			
				</li>
				
				<!-- TKKN_Savanna -->
			
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="TKKN_Savanna"]/wildAnimals</xpath>
					<value>
						<ESCP_Welwa>0.05</ESCP_Welwa>
					</value>	
				</li>
		
			</operations>
		</match>
	</Operation>
		
</Patch>

