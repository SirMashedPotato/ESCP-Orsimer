<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Patch for Recipes_Genes -->

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Genetic Rim</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesMuffalo"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>MorrowRim_Echatere</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
	
</Patch>

