<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ESCP_LeaderTitleMaker_Orsimeri</defName>
    <include>
      <li>LeaderTitleMaker_GlobalStructure</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>memeLeaderNoun(priority=1)->[cultureLeaderNoun]</li> <!-- ignore all meme nouns, tribals always use a culture noun -->

        <li>cultureLeaderTitle->[cultureLeaderAdjective] [cultureLeaderNoun]</li>

        <li>cultureLeaderAdjective->great</li>
        <li>cultureLeaderAdjective->high</li>
        <li>cultureLeaderAdjective->first</li>
        <li>cultureLeaderAdjective->grand</li>

        <li>cultureLeaderNoun->chieftain</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>ESCP_NamerDeity_Orsimeri</defName>
    <rulePack>
      <rulesStrings>
        <li>r_deityName->[OrsimerGods]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>OrsimerGods->ESCP_OrsimerNames/OrsimerGods</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ESCP_NamerDeityType_Orsimeri</defName>
    <rulePack>
      <rulesStrings>
        <li>r_deityType->[OrsimerGodsType]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>OrsimerGodsType->ESCP_OrsimerNames/OrsimerGodsType</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
</Defs>
