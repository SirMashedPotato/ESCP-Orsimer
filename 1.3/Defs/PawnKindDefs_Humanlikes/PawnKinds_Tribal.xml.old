<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <PawnKindDef Name="OrsimerTribalBase" Abstract="True">
    <race>MorrowRim_OrsimerRace</race>
    <defaultFactionType>OrsimerTribeCivil</defaultFactionType>
    <maxGenerationAge>60</maxGenerationAge>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <invNutrition>2.55</invNutrition>
    <invFoodDef>Pemmican</invFoodDef>
    <apparelTags>
      <li>Neolithic</li>
      <li>NeolithicBasic</li>
      <li>OrsimerTotem_Basic</li>
    </apparelTags>
	<initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>12~25</initialResistanceRange>
  </PawnKindDef>

  <!-- Low tier - archer -->

  <PawnKindDef ParentName="OrsimerTribalBase">
    <defName>OrsimerTribal_Archer</defName>
    <label>archer</label>
    <combatPower>48</combatPower>
    <itemQuality>Poor</itemQuality>
    <gearHealthRange>0.5~1.8</gearHealthRange>
    <apparelMoney>180~350</apparelMoney>
    <weaponMoney>80~80</weaponMoney>
    <weaponTags>
      <li>NeolithicRangedBasic</li>
    </weaponTags>
	<disallowedTraits>
      <li>Brawler</li>
    </disallowedTraits>
    <techHediffsMoney>50~50</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <techHediffsChance>0.03</techHediffsChance>
  </PawnKindDef>

  <!-- Mid tier - warrior, hunter, trader -->
  
  <PawnKindDef ParentName="OrsimerTribalBase">
    <defName>OrsimerTribal_Warrior</defName>
    <label>warrior</label>
    <combatPower>60</combatPower>
    <itemQuality>Poor</itemQuality>
    <canBeSapper>true</canBeSapper>
    <gearHealthRange>0.5~1.8</gearHealthRange>
    <apparelMoney>200~300</apparelMoney>
    <apparelRequired>
      <li>MorrowRim_Apparel_OrcishHelmet</li>
      <li>MorrowRim_Apparel_OrcishArmor</li>
    </apparelRequired>
    <weaponMoney>150~250</weaponMoney>
    <weaponTags>
      <li>OrsimerWeapon</li>
    </weaponTags>
  </PawnKindDef>

  <PawnKindDef ParentName="OrsimerTribalBase" Name="OrsimerTribal_Hunter">
    <defName>OrsimerTribal_Hunter</defName>
    <label>hunter</label>
    <combatPower>65</combatPower>
    <gearHealthRange>0.5~1.8</gearHealthRange>
    <apparelMoney>200~300</apparelMoney>
    <apparelRequired>
      <li>Apparel_WarVeil</li>
    </apparelRequired>
    <weaponMoney>100~100</weaponMoney>
    <weaponTags>
      <li>NeolithicRangedDecent</li>
    </weaponTags>
  </PawnKindDef>
  
  <PawnKindDef ParentName="OrsimerTribal_Hunter">
    <defName>OrsimerTribal_Trader</defName>
    <label>trader</label>
    <trader>true</trader>
    <apparelRequired Inherit="False">
    </apparelRequired>
  </PawnKindDef>

  <!-- High tier - berserker, heavy archer, chief melee, chief ranged -->

  <PawnKindDef ParentName="OrsimerTribalBase">
    <defName>OrsimerTribal_Berserker</defName>
    <label>beserker</label>
    <combatPower>90</combatPower>
    <canBeSapper>true</canBeSapper>
    <apparelMoney>200~550</apparelMoney>
    <apparelRequired>
      <li>MorrowRim_Apparel_OrcishFullHelmet</li>
      <li>MorrowRim_Apparel_OrcishArmor</li>
	  <li>MorrowRim_Apparel_TotemWroth</li>
    </apparelRequired>
    <weaponMoney>300~500</weaponMoney>
    <weaponTags>
      <li>OrsimerWeaponLarge</li>
    </weaponTags>
	<initialWillRange>3~5</initialWillRange>
    <initialResistanceRange>19~30</initialResistanceRange>
  </PawnKindDef>
  
  <!-- also has shield -->
